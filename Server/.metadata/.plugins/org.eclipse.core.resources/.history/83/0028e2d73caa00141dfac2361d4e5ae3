package evans.dave.Androtag.Common;


public class TCPMessage {
	
	public static final byte GET_GAMES = 1; // CS game list pull
	
	public static final byte REQ_JOIN = 2;  // CS Request to join game
	
	public static final byte REQ_JOIN_TEAM = 3;  // CS Request to join team in game
	
	public static final byte GET_GAME_INFO = 4; // CS Get gameinfo for a specific id
	
	public static final byte GAME_UPDATE = 5; // CS game update (deaths, isDead, messages)
	                                          // SC game update (teams, isOver, messages)S
	

}
